#!/bin/sh
set -o posix

if [ -e "$HOME"/.Kibrary/bin/.kibraryrc ]; then
  . "$HOME"/.Kibrary/bin/.kibraryrc -- || return 1
else
  printf "No file for variables. Please reinstall Kibrary.\n"
  exit 71
fi

if ! javaCheck -r >/dev/null; then
  javaCheck 
  exit 1
fi

"$JAVA" io.github.kensuke1984.kibrary.Property "$@"
exit $?
