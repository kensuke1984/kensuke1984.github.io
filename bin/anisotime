#!/bin/bash

if [ -z "${JAVA}" ] ; then
JAVA=java 
fi

KIBRARYHOME="$HOME/.Kibrary"
kibin="$KIBRARYHOME/bin"
kibrary=$kibin/$(ls $kibin/kib*jar | xargs -i basename {} )

$kibin/javaCheck > /dev/null

if [ $? -ne 0 ]; then
$kibin/javaCheck 
exit 1
fi

$JAVA -cp $kibrary io.github.kensuke1984.anisotime.ANISOtime $@


